%p
  %b Submitter:
  = @review.submitter.name if @review.submitter
%p
  %b Project:
  = @review.project.name
%p
  %b Development branch:
  = @review.development_branch
%p
  %b Close date:
  = @review.close_date
%p
  %b Votes:
%table
  %tr
    %th User
    %th Vote
  - @review.review_votes.each do |vote|
    %tr
      %td= vote.user.name
      %td= vote.display_vote
  

= link_to 'Edit', edit_review_path(@review)
\|
= link_to 'Back', reviews_path
